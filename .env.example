# MongoDB root credentials (used by MongoDB container)
MONGODB_ROOT_USER=admin
MONGODB_ROOT_PASSWORD=admin_password

# Application database credentials (used by both MongoDB init script and Go app)
MONGODB_USER=myapp_user
MONGODB_PASSWORD=secure_password

MONGODB_DATABASE=myapp_dev
GITHUB_USER=my_github_user
GITHUB_TOKEN=my_github_token
REDIS_HOST=redis-dev
REDIS_PORT=6379
